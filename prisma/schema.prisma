generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
  id            Int     @id @default(autoincrement())
  name          String
  salary        String
  currency      String
  onContract    Boolean @default(false)
  department    String
  subDepartment String
}
